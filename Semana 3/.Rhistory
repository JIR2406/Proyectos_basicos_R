library(dplyr)
library(readr)
cacao <- read.csv("flavors_of_cacao.csv")
colnames(cacao)
cacao2 <- cacao %>%
select(Rating,Cocoa.Percent,Company...Maker.if.known.,Company.Location)
cacao2 %>%
